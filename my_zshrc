# shell settings
## zsh auto complement
autoload -U compinit
compinit

## prompt initialize
autoload -U promptinit
promptinit

## use prompt pure
prompt pure

# alias
alias ll="ls -l"
alias la="ls -la"
alias rmf="rm -f"
alias rmr="rm -rf"

# PATH settings
export PATH="$PATH:$HOME/development/flutter/bin"
export PYENV_ROOT="$HOME/.pyenv"
export PATH="$PYENV_ROOT/bin:$PATH"
eval "$(pyenv init -)"

